{
    "name": "jpcaparas/n8n-client",
    "description": "A Laravel library that interfaces with the self-hosted N8N.io REST API",
    "type": "library",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0 || ^8.0",
        "laravel/framework": "^10.0 || ^11.0"
    },
    "require-dev": {
        "laravel/pint": "^1.19",
        "orchestra/testbench": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "JPCaparas\\N8N\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JPCaparas\\N8N\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JPCaparas\\N8N\\N8NServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit",
        "format": "pint",
        "tinker": "testbench tinker"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
